            // 尋找網址列中是否有#
            if (url.indexOf("#") != -1) {
                // 分割字串，把分割後的字串放進陣列中
                var ary1 = url.split("#");
                // ary1 陣列內容為：
                // ary1[0] = 'index.html'
                // ary1[1] = 'QandA01'
                var param = ary1[1];
                if (param == "qa_1") {
                    changeTab("", 0, "qna_menu_item");
                    jsGoAnchor();
                } else if (param == "qa_2") {
                    changeTab("", 1, "qna_menu_item");
                    jsGoAnchor();

    function runSvgThreeIcon(){
      svg_ThreeTimer_1 = setTimeout(function () {
        svg_ThreeTimer_2 = setTimeout(function () {
          svg_ThreeTimer_3 = setTimeout(function () {
            redraw(svg_idThreeIcon3);
          }, 2000);
          redraw(svg_idThreeIcon2);
        }, 2000);
        redraw(svg_idThreeIcon1);
      }, 2000); 
    }
    $('#idAnchor1').on('scrollSpy:enter', function () {
    //先跑一次 3個icon輪播 
      runSvgThreeIcon();
      //每隔6秒再跑一次 3個icon輪播
      svg_ThreeTimer_interval = setInterval(function () {
        runSvgThreeIcon();
      }, 6000);
    }).scrollSpy();

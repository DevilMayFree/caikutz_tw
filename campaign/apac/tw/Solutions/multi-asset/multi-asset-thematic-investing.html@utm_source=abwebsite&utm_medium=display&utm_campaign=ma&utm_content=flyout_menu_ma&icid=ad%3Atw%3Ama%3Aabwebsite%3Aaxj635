    });

    //top選單-mobile
    $('.jsMobMenuToggle').on('click', function () {
      if ($('.jsMobMenuToggle').hasClass('open')) {
        funcCloseMobMenu();
      } else {
        funcOpenMobMenu();
      }
    });

    //top選單 anchor click
    $('.jsGoAnchor').on('click', function () {
      var anchorId = $(this).attr('data-link');
      // console.log('anchorId: ' + anchorId);
      if ($(this).attr('data-link') == 'top') {
        $("html, body").animate({ scrollTop: 0 }, 500);
      } else {
        if ($('.jsMobMenuToggle').hasClass('open')) {
          funcCloseMobMenu();
        }
        if (!$('#idHeader').hasClass('stickyed')) {
          //選單未在sticky狀態時click menu item
          var navStickyOffset = $('#idHeader').outerHeight();
          $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset - navStickyOffset + 2 }, 500);
        } else {
          $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset + 2 }, 500);
        }
        $('.jsGoAnchor').removeClass('-active');
        $('[data-link="' + anchorId + '"]').addClass('-active');
      }
    });

    //section1 多元優勢
    //收合
    $('.jsTgHeader').on('click', function () {
      var myBody = $(this).next('.jsTgBody');
      if ($(this).hasClass('open')) {
        //關起來
        $(this).removeClass('open');
        myBody.removeClass('open');
        myBody.slideUp();
      } else {
        var offsetHeight = 0;
        if ($('.jsTgHeader.open').length > 0) {
          // alert($('.jsTgHeader.open').length);
          //目前有其他開啟的body
          var prevBody = $('.jsTgBody.open');
          var prevHeader = $('.jsTgHeader.open');
          offsetHeight = prevBody.outerHeight();
          //上一個body關起來
          prevBody.slideUp();
          prevHeader.removeClass('open');
          prevBody.removeClass('open');
          if (prevHeader.attr("data-idx") > $(this).attr("data-idx")) {
            //若上一個body在當前body的下面，則不會影響scrolltop，不需計算offset
            // alert("不需要計算offset" + prevHeader.attr("data-idx") + " " + $(this).attr("data-idx"));
            offsetHeight = 0;
          }
        }
        //打開body
        $(this).addClass('open');
        myBody.addClass('open');
        myBody.slideDown();

        var anchorId = $(this).attr("id");

        //捲動到jsTgHeader置頂
        $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset - offsetHeight + 2 }, 500);

        //啟動body內的動畫
        if (anchorId == "idAnchorThreeHead_1") {

        } else if (anchorId == "idAnchorThreeHead_3") {

        }

      }
    });


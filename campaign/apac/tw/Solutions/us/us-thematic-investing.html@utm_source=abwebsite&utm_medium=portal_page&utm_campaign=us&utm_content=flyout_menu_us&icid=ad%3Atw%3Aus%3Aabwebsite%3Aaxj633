        type: SVG_VIVUS_ANITYPE,
        duration: SVG_ANIM_DURATION,
        reverseStack: true,
      });
      var svg_idThreeIcon3 = new Vivus('idThreeIcon3', {
        type: SVG_VIVUS_ANITYPE,
        duration: SVG_ANIM_DURATION,
        reverseStack: true,
      });
      var svg_idThreeChart1_1 = new Vivus('idThreeChart1_1', {
        type: SVG_VIVUS_ANITYPE,
        duration: 500,
        reverseStack: true,
      });
      var svg_idThreeChart1_2 = new Vivus('idThreeChart1_2', {
        type: SVG_VIVUS_ANITYPE,
        duration: 500,
        reverseStack: true,
      });
      var svg_idThreeChart1_3 = new Vivus('idThreeChart1_3', {
        type: SVG_VIVUS_ANITYPE,
        duration: 500,
        reverseStack: true,
      });
      var svg_idThreeChart1_1_m = new Vivus('idThreeChart1_1_m', {
        type: SVG_VIVUS_ANITYPE,

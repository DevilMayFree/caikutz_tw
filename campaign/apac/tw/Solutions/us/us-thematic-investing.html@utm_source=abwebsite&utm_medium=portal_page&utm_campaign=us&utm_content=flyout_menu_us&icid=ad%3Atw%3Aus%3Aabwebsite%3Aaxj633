              centeredSlides: false,
            },
            768: {
              slidesPerView: 'auto',
              spaceBetween: 30,
              centeredSlides: false,
            },
            960: {
              slidesPerView: 3,
              spaceBetween: 30,
              centeredSlides: false,
            },
          }
        });

      //美國投資專家
      $('.us-invest-product-wrapper').on('scrollSpy:enter', function () {
        redraw(svg_idInvestPic);
        svg_idInvestPic_interval = setInterval(function () {
          redraw(svg_idInvestPic);
        }, SVG_REDRAW_DURATION);
      }).scrollSpy();

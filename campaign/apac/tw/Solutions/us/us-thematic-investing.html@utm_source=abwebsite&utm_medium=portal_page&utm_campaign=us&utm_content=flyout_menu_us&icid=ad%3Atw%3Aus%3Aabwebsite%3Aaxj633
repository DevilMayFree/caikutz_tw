      $('#idAnchor1').on('scrollSpy:enter', function () {
        svg_ThreeTimer = setInterval(function () {
          setTimeout(function () {
            setTimeout(function () {
              setTimeout(function () {
                redraw(svg_idThreeIcon3);
              }, 2000);
              redraw(svg_idThreeIcon2);
            }, 2000);
            redraw(svg_idThreeIcon1);
          }, 2000);
        }, 6000);
      }).scrollSpy();
      $('#idAnchor1').on('scrollSpy:exit', function () {
        clearInterval(svg_ThreeTimer);
        svg_ThreeTimer = null;
      });
      //GDP佔比
      $('.us-chart-3col').on('scrollSpy:enter', function () {
        if ($(window).width() > WIDTH_BREAKPOINT_PAD) {
          setTimeout(function () {
            redraw(svg_idThreeChart1_1);
            redraw(svg_idThreeChart1_2);
            redraw(svg_idThreeChart1_3);
          }, 500);
        } else {
          setTimeout(function () {
            redraw(svg_idThreeChart1_1_m);
            redraw(svg_idThreeChart1_2_m);
            redraw(svg_idThreeChart1_3_m);
          }, 500);

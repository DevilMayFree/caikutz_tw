        svg_idInvestPic_interval = null;
      });
      //進入視線時，才開始自動輪播
      $('#idSwiperInvestProduct').scrollSpy();
      $('#idSwiperInvestProduct').on('scrollSpy:enter', function () {
        swiper_idSwiperInvestProduct.slideTo(0, 1000, false); //從頭開炲
        swiper_idSwiperInvestProduct.autoplay.start();
      });
      $('#idSwiperInvestProduct').on('scrollSpy:exit', function () {
        swiper_idSwiperInvestProduct.autoplay.stop();
      });

      //美國系列焦點 swiper
      var swiper_idSwiperFocusProduct = new Swiper('#idSwiperFocusProduct',
        {
          watchOverflow: true,
          spaceBetween: 0,
          centeredSlides: false,
          loop: false,
          autoplay: {
            delay: 4000,
            disableOnInteraction: true
          },
          pagination: {
            clickable: true,
            el: '.idSwiperFocusPagination',
            type: 'bullets',
          },
          breakpoints: {
            1: {
              slidesPerView: 'auto',
              spaceBetween: 20,
              centeredSlides: false,
            },
            960: {
              slidesPerView: 3,
              spaceBetween: 20,
              centeredSlides: false,
            },
            1150: {
              slidesPerView: 3,
              spaceBetween: 30,
              centeredSlides: false,
            },
          }
        });
      //進入視線時，才開始自動輪播
      $('#idSwiperFocusProduct').scrollSpy();
      $('#idSwiperFocusProduct').on('scrollSpy:enter', function () {
        swiper_idSwiperFocusProduct.slideTo(0, 1000, false); //從頭開炲
        swiper_idSwiperFocusProduct.autoplay.start();
      });
      $('#idSwiperFocusProduct').on('scrollSpy:exit', function () {
        swiper_idSwiperFocusProduct.autoplay.stop();
      });





    });//ready


    /*****************************************************************/
    /* Google IFrame Player API */
    /*****************************************************************/

    // 載入 Google IFrame Player API
    var scriptUrl = 'https:\/\/www.youtube.com\/s\/player\/d2ff46c3\/www-widgetapi.vflset\/www-widgetapi.js';
    if (!window["YT"]) var YT = {
      loading: 0,
      loaded: 0
    };
    if (!window["YTConfig"]) var YTConfig = {
      "host": "https://www.youtube.com"
    };
    if (!YT.loading) {
      YT.loading = 1;
      (function () {
        var l = [];
        YT.ready = function (f) {
          if (YT.loaded) f();
          else l.push(f)
        };
        window.onYTReady = function () {
          YT.loaded = 1;
          for (var i = 0; i < l.length; i++) try {
            l[i]()
          } catch (e) { }
        };
        YT.setConfig = function (c) {
          for (var k in c)
            if (c.hasOwnProperty(k)) YTConfig[k] = c[k]
        };
        var a = document.createElement("script");
        a.type = "text/javascript";
        a.id = "www-widgetapi-script";
        a.src = scriptUrl;
        a.async = true;
        var c = document.currentScript;
        if (c) {
          var n = c.nonce || c.getAttribute("nonce");
          if (n) a.setAttribute("nonce", n)
        }
        var b =
          document.getElementsByTagName("script")[0];
        b.parentNode.insertBefore(a, b)
      })()
    };

    // 載入youtube影片
    var playerVideoPC;

    function onYouTubeIframeAPIReady() {
      playerVideoPC = new YT.Player('videoPC', {
        height: '390', // 高度預設值為390，css會調成responsive
        width: '640', // 寬度預設值為640，css會調成responsive
        videoId: 'X3eS9bxYPLw',
        events: {
          'onStateChange': onPlayerStateChange
        }
      });

    }
    // 設定影片的 tracking code
    function onPlayerStateChange(e) {
      var data = e.data;
      if (data == '-1') {
        gtag('event', 'click', {
          'event_category': 'button',
          'event_label': 'US-video'
        });
      }
    }
    //捲動
    $(document).scroll(function () {

      var scrollDistance = $(this).scrollTop();
      //   console.log('scrollDistance: ' + scrollDistance);

      //收起mobile menu
      if ($('.jsMobMenuToggle').hasClass('open')) {
        funcCloseMobMenu();
      }
      //pc sticky menu 出現
      if (scrollDistance > navHeight) {
        $('#idHeader').addClass('stickyed');
      } else {
        $('#idHeader').removeClass('stickyed');
      }
      if (scrollDistance > navHeight * 3) { // scroll 加入滑入效果
        $('#idHeader').addClass('inview');
      } else {
        $('#idHeader').removeClass('inview');
      }

      //選單項目 active
      if (scrollDistance > ($('#idAnchor1').offset().top - topMenuOffset)
        && scrollDistance <= ($('#idAnchor1_end').offset().top - topMenuOffset)) {
        $('.jsGoAnchor').removeClass('-active');
        $('[data-link="idAnchor1"]').addClass('-active');
      }
      else if (scrollDistance > ($('#idAnchor2').offset().top - topMenuOffset)
        && scrollDistance <= ($('#idAnchor2_end').offset().top - topMenuOffset)) {
        $('.jsGoAnchor').removeClass('-active');
        $('[data-link="idAnchor2"]').addClass('-active');
      }
      else if (scrollDistance > ($('#idAnchor3').offset().top - topMenuOffset)
        && scrollDistance <= ($('#idAnchor3_end').offset().top - topMenuOffset)) {
        $('.jsGoAnchor').removeClass('-active');
        $('[data-link="idAnchor3"]').addClass('-active');
      }
      else {
        $('.jsGoAnchor').removeClass('-active');
      }
    });

  </script>

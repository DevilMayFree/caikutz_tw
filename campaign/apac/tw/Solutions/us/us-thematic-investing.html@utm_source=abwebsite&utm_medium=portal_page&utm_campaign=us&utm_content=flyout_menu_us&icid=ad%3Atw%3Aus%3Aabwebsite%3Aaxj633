        clearInterval(svg_idFooterLine_interval);
        svg_idFooterLine_interval = null;
      });


      //animation - end


      //initialize
      navHeight = $('#idHeader').outerHeight();
      topMenuOffset = navHeight;
      $(window).resize(function () {
        navHeight = $('#idHeader').outerHeight();
        topMenuOffset = navHeight;
      });

      //top選單-mobile
      $('.jsMobMenuToggle').on('click', function () {
        if ($('.jsMobMenuToggle').hasClass('open')) {
          funcCloseMobMenu();
        } else {
          funcOpenMobMenu();
        }
      });

      //top選單 anchor click
      $('.jsGoAnchor').on('click', function () {
        var anchorId = $(this).attr('data-link');
        // console.log('anchorId: ' + anchorId);
        if ($(this).attr('data-link') == 'top') {
          $("html, body").animate({ scrollTop: 0 }, 500);
        } else {
          if ($('.jsMobMenuToggle').hasClass('open')) {
            funcCloseMobMenu();
          }
          if (!$('#idHeader').hasClass('stickyed')) {
            //選單未在sticky狀態時click menu item
            var navStickyOffset = $('#idHeader').outerHeight();
            $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset - navStickyOffset + 2 }, 500);
          } else {
            $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset + 2 }, 500);
          }
          $('.jsGoAnchor').removeClass('-active');
          $('[data-link="' + anchorId + '"]').addClass('-active');
        }
      });

      //美國投資版圖
      //收合
      $('.jsTgHeader').on('click', function () {
        var myBody = $(this).next('.jsTgBody');
        if ($(this).hasClass('open')) {
          //關起來
          $(this).removeClass('open');
          myBody.removeClass('open');
          myBody.slideUp();
        } else {
          var offsetHeight = 0;
          if ($('.jsTgHeader.open').length > 0) {
            // alert($('.jsTgHeader.open').length);
            //目前有其他開啟的body
            var prevBody = $('.jsTgBody.open');
            var prevHeader = $('.jsTgHeader.open');
            offsetHeight = prevBody.outerHeight();
            //上一個body關起來
            prevBody.slideUp();
            prevHeader.removeClass('open');
            prevBody.removeClass('open');
            if (prevHeader.attr("data-idx") > $(this).attr("data-idx")) {
              //若上一個body在當前body的下面，則不會影響scrolltop，不需計算offset
              // alert("不需要計算offset" + prevHeader.attr("data-idx") + " " + $(this).attr("data-idx"));
              offsetHeight = 0;
            }
          }
          //打開body
          $(this).addClass('open');
          myBody.addClass('open');
          myBody.slideDown();

          var anchorId = $(this).attr("id");

          //捲動到jsTgHeader置頂
          $("html, body").animate({ scrollTop: $('#' + anchorId).offset().top - topMenuOffset - offsetHeight + 2 }, 500);

          //啟動body內的動畫
          if (anchorId == "idAnchorThreeHead_1") {
            if ($('.us-three-img-pc').css('display') == 'block') {
              redraw(svg_idThreeChart1_1);
              redraw(svg_idThreeChart1_2);
              redraw(svg_idThreeChart1_3);
            } else {
              redraw(svg_idThreeChart1_1_m);
              redraw(svg_idThreeChart1_2_m);
              redraw(svg_idThreeChart1_3_m);
            }

          } else if (anchorId == "idAnchorThreeHead_3") {
            if ($('.us-three-img-pc').css('display') == 'block') {
              redraw(svg_idThreeChart3_map);
              // setTimeout(function () {
              //   redraw(svg_idThreeChart3_usa);
              // }, 2000);            

            } else {
              redraw(svg_idThreeChart3_map_m);
              // setTimeout(function () {
              //   redraw(svg_idThreeChart3_usa_m);
              // }, 2000); 

            }
          }

        }
      });

      //美國投資經驗
      //50年 跑數字
      var COUNTER_NUM = 50;
      $('.jsCounter').scrollSpy();
      $('.jsCounter').on('scrollSpy:enter', function () {
        //Board: counter up numbers
        $(this).prop('Counter', 0).animate({
          // Counter: $(this).text()
          Counter: COUNTER_NUM
        }, {
          duration: 1500,
          easing: 'swing',
          step: function (now) {
            $(this).text(Math.ceil(now));
          }
        });
      });
      //大事紀 loop
      var jsExpLi_timer = null, jsExpLi_interval = 2000;
      var jsExpLi_elem = $('.jsExpLi'); //Elements to loop through
      var jsExpLi_idx = 0; //Start at 0

      function funcActTimeline() {
        //Loop through elements
        $(jsExpLi_elem).each(function (index) {
          if (jsExpLi_idx == index) {
            //Show active element
            $(this).addClass('active');
          } else if (jsExpLi_idx == $(jsExpLi_elem).length) {
            //Show message
            $(this).addClass('active');
            //Reset idx when max number is reached
            jsExpLi_idx = 0;
          } else {
            //Hide all non active elements
            $(this).removeClass('active');
          }
        });
        jsExpLi_idx++;
      }
      function funcStartTimeline() {
        funcActTimeline(); //Run once the first time
        //loop
        jsExpLi_timer = setInterval(function () {
          funcActTimeline();
        }, jsExpLi_interval);
      }
      function funcStopTimeline() {
        //stop loop and clear
        clearInterval(jsExpLi_timer);
        jsExpLi_timer = null;
        $('.jsExpLi').removeClass('active');
      }
      var LOTTE_TEST_MODE = false;
      if (LOTTE_TEST_MODE) {
        $('#idExpTimeline').scrollSpy();
        $('#idExpTimeline').on('scrollSpy:exit', function () {
          $('.jsExpLi').removeClass('active');
        });
        $('.jsExpLi').on('click', function () {
          $(this).toggleClass('active');
        });
      } else {
        $('#idExpTimeline').scrollSpy();
        $('#idExpTimeline').on('scrollSpy:enter', function () {
          //loop timeline from 0
          jsExpLi_idx = 0;
          funcStartTimeline();
        });
        $('#idExpTimeline').on('scrollSpy:exit', function () {
          funcStopTimeline();
        });
        // when click, reset idx
        $('.jsExpLi').on('click', function () {
          funcStopTimeline();
          jsExpLi_idx = parseInt($(this).attr("data-idx"));
          funcStartTimeline();
        });
      }
      //美國投資專家 pie chart
      var jsPie_timer = null, jsPie_interval = 2000;
      var jsPieCurActive = "jsPieRight";
      if (!LOTTE_TEST_MODE) {
        jsPie_timer = setInterval(function () {
          // console.log(jsPieCurActive);
          //Loop through elements
          if (jsPieCurActive == "jsPieRight") {
            //active Right
            $('.jsPieLeft').addClass('active');
            $('.jsPieRight').removeClass('active');
            jsPieCurActive = "jsPieLeft";
          } else {
            //active Left
            $('.jsPieRight').addClass('active');
            $('.jsPieLeft').removeClass('active');
            jsPieCurActive = "jsPieRight";
          }
        }, jsPie_interval);
      }
      //美國投資專家 swiper
      var swiper_idSwiperInvestProduct = new Swiper('#idSwiperInvestProduct',
        {
          watchOverflow: true,
          spaceBetween: 0,
          centeredSlides: false,
          loop: false,
          // autoplay: {

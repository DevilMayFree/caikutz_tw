    const WIDTH_BREAKPOINT_MOB = 640;
    function funcCloseMobMenu() {
      $('.jsMobMenuToggle, .jsMobMenuHeader').removeClass('open');
      $('.jsMobMenu').fadeOut(300);
    }
    function funcOpenMobMenu() {
      $('.jsMobMenuToggle, .jsMobMenuHeader').addClass('open');
      $('.jsMobMenu').fadeIn(300);
    }
    function redraw(icon) {
      icon.reset();
      icon.play(1);
    }

    $(document).ready(function () {

      window.onload = function () {
        setTimeout(function () {
          $('#loading').addClass('close'); //左刷特效
          setTimeout(function () {
            $("#loading").hide(); //hide loading
            wow = new WOW(
              { animateClass: 'animated', offset: 100 }
            );
            wow.init();
          }, 100);
        }, 1500);
      };
      //強強聯手
      $(window).resize(function () {
        // console.log('resize called');
        var width = $(window).width();
        if (width <= 640) {
          //animation using bgFadeInRight300-mobile
          $('.us-honor-article').removeClass('bgFadeInRight300').addClass('bgFadeInRight300-mobile');
          $('.us-honor-article').attr("data-anim", "bgFadeInRight300-mobile");
        }
        else {
          $('.us-honor-article').removeClass('bgFadeInRight300-mobile').addClass('bgFadeInRight300');
          $('.us-honor-article').attr("data-anim", "bgFadeInRight300");
        }
      })
        .resize();//trigger the resize event on page load.  

      //play animation
      if (true) {
        $('.jsWowRepeat').on('scrollSpy:exit', function () {
          $(this).css({
            'visibility': 'hidden',
            'animation-name': 'none'
          }).removeClass('animated');
        }).scrollSpy();
        $('.jsWowRepeat').on('scrollSpy:enter', function () {
          $(this).css({
            'visibility': 'visible',
            'animation-name': $(this).attr('data-anim')
          }).addClass('animated');
        })
      }

      //animation - start
      var svg_idThreeIcon1 = new Vivus('idThreeIcon1', {
        type: SVG_VIVUS_ANITYPE,
        duration: SVG_ANIM_DURATION,
        reverseStack: true,
      });

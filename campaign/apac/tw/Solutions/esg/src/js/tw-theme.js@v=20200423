\n        } else {\n          // Safari\n          device_info.browser = 'safari';\n        }\n      } else {\n        device_info.browser = 'other';\n      }\n    }\n  }\n\n  return device_info;\n}\n\n/* harmony default export */ __webpack_exports__[\"default\"] = (detectDevice);\n\n//# sourceURL=webpack:///./src/js/helpers/detectDevice.js?");

